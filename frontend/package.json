{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "cz": "git-cz",
    "start": "next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@storybook/react": "^7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "commitizen": "^4",
    "eslint": "^9",
    "eslint-config-next": "^15.3.4",
    "husky": "^8",
    "jest": "^29",
    "lint-staged": "^15",
    "prettier": "^3",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,json}": [
      "npm run lint",
      "npm run format:check"
    ]
  }
}
